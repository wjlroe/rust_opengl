{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cargo",
      "label": "cargo run",
      "subcommand": "run",
      "problemMatcher": [
        "$rustc"
      ],
      "group": "build"
    },
    {
      "type": "cargo",
      "subcommand": "build",
      "label": "cargo build",
      "problemMatcher": [
        "$rustc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
